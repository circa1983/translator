{
    "name": "circa1983/translator",
    "description": "A translation field for Craft CMS ",
    "type": "craft-plugin",
    "version": "1.0.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translator"
    ],
    "support": {
        "docs": "https://github.com/circa1983/translator#readme",
        "issues": "https://github.com/circa1983/translator/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jan D'Hollander",
            "homepage": "https://www.thebasement.be/"
        },
        {
            "name": "Sam Walker",
            "homepage": "https://www.meandhimdesign.co.uk/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
            "circa1983\\translator\\": "src/"
        }
    },
    "extra": {
        "name": "Translator",
        "handle": "translator",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "???",
        "class": "circa1983\\translator\\Translator"
    }
}
